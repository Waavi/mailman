{
    "name": "waavi/mailman",
    "description": "Easier email sending for Laravel 5 with CSS inlining.",
    "keywords": [
        "waavi",
        "laravel",
        "email",
        "mailman",
        "mailer"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Waavi",
        "email": "info@waavi.com",
        "homepage": "http://waavi.com"
    }],
    "require": {
        "tijsverkoyen/css-to-inline-styles": "2.*",
        "illuminate/view": ">=5.1 <5.7",
        "illuminate/support": ">=5.1 <5.7",
        "illuminate/queue": ">=5.1 <5.7",
        "illuminate/mail": ">=5.1 <5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4",
        "orchestra/testbench": "^3.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\Mailman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\Mailman\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
